	<%!
	static{
		System.out.println("正在装载由JSP生成的Servlet!");
	}
	private int globalCount = 0;
	//覆盖jspInit方法
	public void jspInit(){
		System.out.println("正在初始化JSP!");
	}
	//覆盖jspDestroy方法
	public void jspDestroy(){
		System.out.println("JSP已经被销毁！");
	}
	%>

</body>
